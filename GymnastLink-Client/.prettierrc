{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "bracketSpacing": false,
  "printWidth": 120,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^@mui/(.*)$",
    "^@assets(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^@customTypes/(.*)$",
    "^@contexts/(.*)$",
    "^@enums/(.*)$",
    "^@utils/(.*)$",
    "^@styles/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true
}
